#include <stdio.h>
#include <stdlib.h>

int manin(){

    char grade = 'A';
    
    switch(grade){
    case 'A' :
        printf("You did great! ");
        break;
    case 'B' :
        printf("You did alright! ");
        break;
    case 'C' :
        printf("You did poorly");
        break;
    case 'D' :
        printf("You did very bad");
        break;
    case 'F' :
        printf("You Falied! ");
        break;
    default :
        printf("Invalid Grade");
    }
    
    return 0;
}
